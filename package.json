{
  "name": "react-mobx-vm",
  "version": "1.0.0",
  "description": "View & Model are mixed now! via react mobx",
  "main": "index.js",
  "scripts": {
    "build": "babel src/ -d dist/ -D",
    "watch": "npm run build -- -w",
    "prepare": "npm run build",
    "test": "jest",
    "test:cy:ci": "npm-run-all --race --parallel \"gojs -- -s\" \"cy:run -- --record --key {1}\" --",
    "test:cy:run": "run-p --race \"gojs -- -s\" cy:run",
    "test:cy:open": "run-p -l gojs cy:open",
    "gojs": "gojs examples --noopen -p 8001",
    "cy:run": "cypress run",
    "cy:open": "cypress open"
  },
  "keywords": [
    "imcuttle",
    "View & Model",
    "react-mobx-vm"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "mobx-react": "^4.0.0",
    "mobx": "^3.0.0",
    "react": "^15.0.0",
    "prop-types": "^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cypress": "^1.4.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-react": "^7.6.1",
    "go-js": "^1.3.8",
    "history": "^3.3.0",
    "jest": "^22.3.0",
    "lodash": "^4.17.5",
    "mobx": "^3.5.1",
    "mobx-react": "^4.3.5",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^3.2.0",
    "sinon": "^4.3.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup/react.js",
    "coverageReporters": [
      "json",
      "text",
      "lcov"
    ]
  }
}
