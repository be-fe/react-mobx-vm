{
  "name": "react-mobx-vm",
  "version": "0.0.4",
  "description": "View & Model are mixed now via react mobx",
  "scripts": {
    "babel": "rm -rf lib/ && cross-env BABEL_ENV=babel babel src/ -d lib/ -D",
    "watch": "npm run babel -- -w",
    "rollup": "rollup -c --silent",
    "prepare": "run-p babel rollup",
    "test": "jest",
    "test:cy:ci": "npm-run-all --race --parallel \"gojs -- -s\" \"cy:run -- --record --key {1}\" --",
    "test:cy:run": "run-p --race \"gojs -- -s\" cy:run",
    "test:cy:open": "run-p gojs cy:open",
    "gojs": "gojs examples --noopen -p 8001",
    "gojs:open": "gojs examples -p 8001",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "clean-picidae": "rm -rf node_modules/picidae/node_modules/{react,react-dom,react-router,history}",
    "picidae": "picidae start",
    "lint": "eslint ./src --fix",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "module": "dist/franxx.es.js",
  "jsnext:main": "dist/franxx.es.js",
  "cdn": "dist/react-mobx-vm.min.js",
  "unpkg": "dist/react-mobx-vm.min.js",
  "jsdelivr": "dist/react-mobx-vm.min.js",
  "files": [
    "lib",
    "dist"
  ],
  "keywords": [
    "imcuttle",
    "View & Model",
    "react-mobx-vm"
  ],
  "author": "imcuttle",
  "license": "MIT",
  "peerDependencies": {
    "mobx-react": "*",
    "mobx": "*",
    "react": "*",
    "prop-types": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.0",
    "@commitlint/config-conventional": "^6.1.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "conventional-changelog-cli": "^1.3.14",
    "create-react-class": "^15.6.3",
    "cross-env": "^5.1.3",
    "cypress": "^1.4.2",
    "cz-conventional-changelog": "^2.1.0",
    "documentation-fork": "^5.4.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-react": "^7.6.1",
    "go-js": "^1.3.11",
    "history": "^3.3.0",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "lint-staged": "^7.0.0",
    "lodash": "^4.17.5",
    "minimist": "^1.2.0",
    "mobx": "^3.5.1",
    "mobx-react": "^4.3.5",
    "mobx-state-tree": "^1.3.1",
    "npm-run-all": "^4.1.2",
    "picidae": "^2.1.18",
    "picidae-theme-haier": "0.0.7",
    "picidae-transformer-exec": "0.0.0",
    "picidae-transformer-file-syntax": "^0.1.3",
    "picidae-transformer-react-render": "^1.2.10",
    "picidae-transformer-toc": "^1.1.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^3.2.0",
    "rollup": "^0.56.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sinon": "^4.3.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup/react.js",
    "coverageReporters": [
      "json",
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "qs": "^6.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imcuttle/react-mobx-vm.git"
  },
  "bugs": {
    "url": "https://github.com/imcuttle/react-mobx-vm/issues"
  },
  "homepage": "https://github.com/imcuttle/react-mobx-vm#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
