{
  "env": {
  },
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6",
          "browsers": ["ie>=9"]
        },
        "loose": true,
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "external-helpers",
    [
      "transform-react-jsx", {
        "pragma": "h"
      }
    ],
    "lodash",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-export-extensions",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": false
      }
    ]
  ],
  "ignore": [
  ]
}
